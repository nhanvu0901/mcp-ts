{
  "name": "fastify-mcp-rag",
  "version": "1.0.0",
  "description": "TypeScript/Fastify application with LangGraph and MCP integration",
  "main": "dist/main.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules -r tsconfig-paths/register src/main.ts ",
    "build": "rimraf dist && tsc --project tsconfig.mcp.json",
    "build:old": "rimraf dist && tsc",
    "start": "node dist/main.js",
    "start:dev": "npm run build && npm run start",
    "clean": "rimraf dist",
    "type-check": "tsc --project tsconfig.mcp.json --noEmit",
    "type-check:old": "tsc --noEmit",
    "lint": "eslint src/main.ts src/controllers/**/*.ts src/types/**/*.ts",
    "lint:fix": "eslint src/main.ts src/controllers/**/*.ts src/types/**/*.ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "docker:build": "docker build -t fastify-mcp-rag .",
    "docker:run": "docker run -p 3000:3000 fastify-mcp-rag",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "keywords": [
    "fastify",
    "typescript",
    "langchain",
    "langgraph",
    "mcp",
    "rag",
    "azure-openai",
    "document-processing"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/compress": "^8.0.3",
    "@fastify/cors": "^11.0.1",
    "@fastify/helmet": "^13.0.1",
    "@fastify/multipart": "^9.0.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@langchain/community": "^0.3.47",
    "@langchain/core": "^0.3.62",
    "@langchain/langgraph": "^0.3.6",
    "@langchain/mcp-adapters": "^0.5.2",
    "@langchain/mongodb": "^0.1.0",
    "@langchain/openai": "^0.5.18",
    "dotenv": "^16.4.7",
    "fastify": "^5.4.0",
    "fastify-decorators": "^3.16.1",
    "fastify-plugin": "^5.0.1",
    "langchain": "^0.3.29",
    "mongodb": "^6.17.0",
    "reflect-metadata": "^0.2.2",
    "uuid": "^10.0.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.14",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "resolutions": {
    "@langchain/core": "^0.3.62"
  },
  "overrides": {
    "@langchain/core": "^0.3.62"
  },
  "pnpm": {
    "overrides": {
      "@langchain/core": "^0.3.62"
    }
  }
}
